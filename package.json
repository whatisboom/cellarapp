{
  "author": {
    "name": "Brandon Jernigan",
    "email": "brandon@whatisboom.com",
    "url": "http://whatisboom.com/"
  },
  "name": "beer-cellar",
  "displayName": "Beer Cellar",
  "repository": {
    "type": "git",
    "url": "https://github.com/whatisboom/cellarapp"
  },
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && ./node_modules/.bin/webpack && cp src/server.js dist/server.js",
    "dev": "./node_modules/.bin/webpack --watch",
    "postinstall": "npm run build",
    "start": "node ./dist/server.js",
    "test": "jest",
    "tdd": "jest --watchAll"
  },
  "heroku-run-build-script": true,
  "engines": {
    "node": "11.2.0"
  },
  "dependencies": {
    "@material-ui/core": "3.6.1",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "3.0.0-alpha.28",
    "@reach/router": "1.2.1",
    "express": "4.16.4",
    "final-form": "4.10.0",
    "final-form-material-ui": "0.3.0",
    "normalize.css": "8.0.1",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-final-form": "3.6.7",
    "react-redux": "5.1.1",
    "redux": "4.0.1",
    "validate.js": "0.12.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/polyfill": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/jest": "23.3.9",
    "@types/reach__router": "1.2.2",
    "@types/react": "16.7.3",
    "@types/react-dom": "16.0.9",
    "@types/react-redux": "6.0.9",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "dotenv": "6.2.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "parcel-bundler": "1.10.3",
    "react-hot-loader": "4.6.3",
    "react-testing-library": "5.3.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.1",
    "typescript": "3.1.6",
    "webpack": "4.27.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-hot-middleware": "2.24.3"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/*.spec.(tsx|ts)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
